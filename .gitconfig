[user]
	name = Yuta Morita
	email = yuta.morita@coincheck.com
[core]
	excludesfile = /Users/morrr/.gitignore_global
	editor = mvim -f
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/morrr/.stCommitMsg
